RewriteEngine On

# Details page
RewriteRule ^details_([A-Za-z0-9_=!-]+)/?$ details.php?data=$1 [NC,L]

# Play page
RewriteRule ^play_([A-Za-z0-9_=!-]+)/?$ play.php?data=$1 [NC,L]

# Live streams
RewriteRule ^ts_live_([A-Za-z0-9_=!-]+)\.m3u8/?$ live.php?id=$1 [NC,L]

# JioTV playlist
RewriteRule ^tsjio_playlist\.m3u/?$ playlist.php [NC,L]

# Sony live streams
RewriteRule ^ts_sony_live_([A-Za-z0-9_=!-]+)\.m3u8/?$ s_live.php?id=$1 [NC,L]
